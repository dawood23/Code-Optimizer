  "components": {
    "schemas": {
      "AddMetricRequest": {
        "type": "object",
        "properties": {
          "metricName": {
            "type": "string",
            "nullable": true
          },
          "language": {
            "type": "string",
            "nullable": true
          },
          "timeTaken": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "CodeOptimizationRequest": {
        "type": "object",
        "properties": {
          "language": {
            "type": "string",
            "nullable": true
          },
          "code": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CodeOptimizationResponse": {
        "type": "object",
        "properties": {
          "optimizedCode": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "errorsFound": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "optimizationsApplied": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "GroqRequest": {
